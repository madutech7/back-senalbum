# Server
PORT=8080
APP_CORS_ALLOWED_ORIGINS=http://localhost:4200,https://your-frontend-domain.com
APP_FRONTEND_URL=https://your-frontend-domain.com

# JWT
APP_JWT_SECRET=default_secret_please_change_this_to_a_secure_random_string_min_32_chars

# Database
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/your_db
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=password

# PayDunya (Payment)
PAYDUNYA_MASTER_KEY=your_master_key
PAYDUNYA_PUBLIC_KEY=your_public_key
PAYDUNYA_PRIVATE_KEY=your_private_key
PAYDUNYA_TOKEN=your_token
PAYDUNYA_MODE=test

# Google OAuth2
SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_GOOGLE_CLIENT_ID=your-google-client-id
SPRING_SECURITY_OAUTH2_CLIENT_REGISTRATION_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Wasabi Storage
WASABI_ENDPOINT=https://s3.eu-west-2.wasabisys.com
WASABI_REGION=eu-west-2
WASABI_BUCKET=your-bucket-name
WASABI_ACCESS_KEY=your-access-key
WASABI_SECRET_KEY=your-secret-key

# Email
SPRING_MAIL_USERNAME=your-email@gmail.com
SPRING_MAIL_PASSWORD=your-app-password
SPRING_MAIL_FROM=your-email@gmail.com